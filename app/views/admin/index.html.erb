<h1>Users</h1>
<li><%=link_to "Apartment Listings", apartments_path%></li>
<table>
  <thead>
    <thead>
      <tr>
        <th>Email</th><th>First Role</th><th>Last Role</th>
      </tr>
    </thead>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.roles.first.name %></td>
      <td><%= user.roles.last.name %></td>
      <td>
        <% if user.email != "ajnavarro1424@gmail.com" %>
        <%= form_tag("/admin/" + user.id.to_s + "?act=grant", method: :PUT) do %>
          <input type="submit" value="Grant Unbridled Power"/>
        <% end %>
      </td>
      <td>
          <%= form_tag("/admin/" + user.id.to_s + "?act=strip", method: :PUT) do %>
          <input type="submit" value="Strip Unbridled Power"/>
          <% end %>
        <% end %>

      </td>
    </tr>
  <% end %>
  </tbody>
</table>
